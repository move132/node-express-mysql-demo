<!DOCTYPE html>
<html lang="zh">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <meta http-equiv="X-UA-Compatible" content="ie=edge" />
        <title>ejs template</title> 
        <link rel="stylesheet" href="./css/bootstrap.min.css" />
        <script src="./js/jquery.min.js"></script>
        <script src="./js/bootstrap.min.js"></script>
        <style>
            .box {
                width: 1280px;
                margin: 0 auto;
            }
            .mtb {
                margin: 20px 0;
            }
            .tooltip-inner{
                max-width: 400px; 
                text-align: left;
            }
            table td .text{ 
                max-width: 400px; 
                overflow : hidden;
                text-overflow: ellipsis;
                display: -webkit-box;
                -webkit-line-clamp: 2;
                -webkit-box-orient: vertical;
            }
        </style>
        <script>
            $(function(){
                $('[data-toggle="tooltip"]').tooltip()
            })
        </script>
    </head>
    <body>
        <div class="box"> 
            <form class="form-inline mtb">
                <div class="form-group">
                    <label for="message">msg</label>
                    <input type="text" class="form-control input-sm" id="message" placeholder="message">        
                </div>
                <button type="button" class="btn btn-primary btn-sm">查询</button>
                <button type="button" class="btn btn-success btn-sm">新增</button>
                <button type="button" class="btn btn-info btn-sm">修改</button>
                <button type="button" class="btn btn-warning btn-sm">删除</button>
                <button type="button" class="btn btn-danger btn-sm">批量删除</button>
            </form>

            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th style="width:50px;">#</th>
                        <th style="width:100px;">项目名称</th>
                        <th style="width:100px;">错误类型</th>
                        <th>错误地址</th>
                        <th style="">错误信息</th>
                        <th style="">错误堆栈</th>
                        <th style="width:150px;">错误时间</th>
                    </tr>
                </thead>
                <tbody>
                    <% console.log(data); data.forEach(function(item,index){ %>
                    <tr class="<%=index % 2==0?'info':'primary' %>">
                        <td  style="width:50px;">
                            <input type="checkbox" value=<%=item.id %>>
                        </td>
                        <td  style="width:100px;">
                            <%=item.projectName %>
                        </td>
                        <td  style="width:100px;">
                            <%=item.type %>
                        </td>
                        <td>
                            <div class="text" data-toggle="tooltip" data-placement="top" title="<%=item.url %>"><%=item.url %></div>
                        </td>
                        <td >
                            <div class="text" data-toggle="tooltip" data-placement="top" title="<%=item.msg %>"><%=item.msg %></div>
                        </td>
                        <td  style="width:100px;">
                            <%=item.statck %>
                        </td>
                        <td style="width:150px;">
                            <%=item.time %>
                        </td>
                    </tr>
                    <% }); %>
                </tbody>
            </table>
        </div>
    </body>
</html>
